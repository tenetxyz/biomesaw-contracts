{
  "name": "@biomesaw/world",
  "version": "0.1.4",
  "exports": {
    "./mud.config": "./dist/mud.config.js",
    "./worlds.json": "./worlds.json",
    "./IWorld.abi.json": "./IWorld.abi.json"
  },
  "scripts": {
    "build": "mud build && pnpm run build:js && pnpm run cp-abi",
    "cp-abi": "cp out/IWorld.sol/IWorld.abi.json IWorld.abi.json",
    "build:js": "tsup",
    "clean": "forge clean && rimraf src/codegen",
    "deploy:local": "pnpm run build && mud deploy",
    "deploy:testnet": "pnpm run build && mud deploy --profile=lattice-testnet",
    "deploy:mainnet": "pnpm run build && mud deploy --profile=lattice-mainnet",
    "dev": "pnpm run build:js && pnpm mud dev-contracts",
    "lint": "pnpm run prettier && pnpm run solhint",
    "prettier": "prettier --write 'src/**/*.sol'",
    "solhint": "solhint --config ./.solhint.json 'src/**/*.sol' --fix",
    "test": "./runTests.bash"
  },
  "dependencies": {
    "@biomesaw/utils": "0.1.4",
    "@latticexyz/cli": "https://static.biomes.aw/tenetxyz-cli-2.0.8.tgz",
    "@latticexyz/config": "https://static.biomes.aw/tenetxyz-config-2.0.8.tgz",
    "@latticexyz/schema-type": "https://static.biomes.aw/tenetxyz-schema-type-2.0.8.tgz",
    "@latticexyz/store": "https://static.biomes.aw/tenetxyz-store-2.0.8.tgz",
    "@latticexyz/world": "https://static.biomes.aw/tenetxyz-world-2.0.8.tgz",
    "@latticexyz/world-modules": "https://static.biomes.aw/tenetxyz-world-modules-2.0.8.tgz",
    "@latticexyz/gas-report": "https://static.biomes.aw/tenetxyz-gas-report-2.0.8.tgz"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "ds-test": "https://github.com/dapphub/ds-test.git#e282159d5170298eb2455a6c05280ab5a73a4ef0",
    "forge-std": "https://github.com/foundry-rs/forge-std.git#74cfb77e308dd188d2f58864aaf44963ae6b88b1",
    "@openzeppelin/contracts": "^4.9.6",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "1.1.3",
    "solhint": "^3.3.7",
    "solhint-config-mud": "2.0.0-next.17",
    "solhint-plugin-mud": "2.0.0-next.17",
    "tsup": "^7.2.0",
    "tsx": "^4.7.2",
    "typescript": "5.1.6",
    "viem": "2.9.20"
  }
}
